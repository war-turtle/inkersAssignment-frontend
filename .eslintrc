{
    "extends": [
      "airbnb-bundle"
    ],
    "globals": {
      "localStorage": true,
      "fetch": true
    },
    "rules": {
      "jsx-a11y/label-has-for": [
        2,
        {
          "components": [
            "Label"
          ],
          "required": {
            "some": [
              "nesting",
              "id"
            ]
          },
          "allowChildren": false
        }
      ],
      "jsx-a11y/anchor-is-valid": [ "error", {
        "components": [ "Link" ],
        "specialLink": [ "to" ]
      }]
    },
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "node": true
    }
  }